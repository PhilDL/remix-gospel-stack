{
  "name": "@remix-gospel-stack/ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:tsup": "tsup --watch",
    "dev:vite": "vite",
    "build": "tsup",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "rm -rf dist",
    "lint": "eslint ./src",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.548.0",
    "npm-run-all2": "^8.0.4",
    "tailwind-merge": "^3.3.1"
  },
  "peerDependencies": {
    "@remix-run/dev": "2.16.0",
    "@remix-run/node": "2.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.2",
    "tailwindcss-animate": "^1.0.6"
  },
  "devDependencies": {
    "@remix-gospel-stack/eslint-config": "workspace:*",
    "@remix-gospel-stack/tsconfig": "workspace:*",
    "@remix-run/dev": "2.17.1",
    "@remix-run/node": "2.17.1",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^9.1.15",
    "@storybook/addon-onboarding": "^9.1.15",
    "@storybook/blocks": "^8.6.14",
    "@storybook/cli": "^9.1.15",
    "@storybook/react": "^9.1.15",
    "@storybook/react-vite": "^9.1.15",
    "@storybook/test": "^8.6.14",
    "@tailwindcss/typography": "^0.5.19",
    "tw-animate-css": "^1.3.8",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "9.38.0",
    "eslint-plugin-storybook": "^9.1.15",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "storybook": "^9.1.15",
    "tailwindcss": "^4.1.16",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./theme.css": "./src/tailwind/theme.css",
    "./components/checkbox": {
      "import": "./src/components/checkbox.tsx",
      "types": "./src/components/checkbox.tsx"
    },
    "./components/button": {
      "import": "./src/components/button.tsx",
      "types": "./src/components/button.tsx"
    },
    "./components/card": {
      "import": "./src/components/card.tsx",
      "types": "./src/components/card.tsx"
    }
  },
  "typesVersions": {
    "*": {
      "button": [
        "src/components/button.tsx"
      ],
      "checkbox": [
        "src/components/checkbox.tsx"
      ],
      "card": [
        "src/components/card.tsx"
      ],
      "components/checkbox": [
        "src/components/checkbox.tsx"
      ],
      "components/button": [
        "src/components/button.tsx"
      ],
      "components/card": [
        "src/components/card.tsx"
      ]
    }
  }
}