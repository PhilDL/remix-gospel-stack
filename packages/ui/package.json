{
  "name": "@remix-gospel-stack/ui",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:tsup": "tsup --watch",
    "dev:vite": "vite",
    "build": "tsup",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "rm -rf dist",
    "lint": "eslint ./src --fix",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.364.0",
    "npm-run-all2": "^6.0.0",
    "tailwind-merge": "^2.2.0"
  },
  "peerDependencies": {
    "@remix-run/dev": "^2.0.0",
    "@remix-run/node": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.2",
    "tailwindcss-animate": "^1.0.6"
  },
  "devDependencies": {
    "@remix-gospel-stack/eslint-config": "workspace:*",
    "@remix-gospel-stack/tsconfig": "workspace:*",
    "@remix-run/dev": "^2.8.0",
    "@remix-run/node": "^2.8.0",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^7.6.7",
    "@storybook/cli": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/react-vite": "^7.6.7",
    "@storybook/test": "^7.6.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint-plugin-storybook": "^0.8.0",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.7",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-tsconfig-paths": "^4.2.3"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts",
      "default": "./dist/index.mjs"
    },
    "./tailwind": {
      "import": "./src/tailwind/index.ts",
      "types": "./src/tailwind/index.ts",
      "default": "./dist/tailwind/index.mjs"
    },
    "./components/checkbox": {
      "import": "./src/components/checkbox.tsx",
      "types": "./src/components/checkbox.tsx"
    },
    "./components/button": {
      "import": "./src/components/button.tsx",
      "types": "./src/components/button.tsx"
    },
    "./components/card": {
      "import": "./src/components/card.tsx",
      "types": "./src/components/card.tsx"
    }
  },
  "typesVersions": {
    "*": {
      "button": [
        "src/components/button.tsx"
      ],
      "checkbox": [
        "src/components/checkbox.tsx"
      ],
      "card": [
        "src/components/card.tsx"
      ],
      "components/checkbox": [
        "src/components/checkbox.tsx"
      ],
      "components/button": [
        "src/components/button.tsx"
      ],
      "components/card": [
        "src/components/card.tsx"
      ]
    }
  }
}