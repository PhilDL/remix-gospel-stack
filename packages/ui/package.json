{
  "name": "@remix-gospel-stack/ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:tsup": "tsup --watch",
    "dev:vite": "vite",
    "build": "tsup",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "rm -rf dist",
    "lint": "eslint ./src",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "storybook": "storybook dev",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.548.0",
    "npm-run-all2": "^8.0.4",
    "tailwind-merge": "catalog:tailwindcss"
  },
  "peerDependencies": {
    "@react-router/dev": "catalog:react-router",
    "@react-router/node": "catalog:react-router",
    "react": "catalog:react19",
    "react-dom": "catalog:react19",
    "tailwindcss": "catalog:tailwindcss",
    "tw-animate-css": "catalog:tailwindcss"
  },
  "devDependencies": {
    "@react-router/dev": "catalog:react-router",
    "@react-router/node": "catalog:react-router",
    "@remix-gospel-stack/eslint-config": "workspace:*",
    "@remix-gospel-stack/tsconfig": "workspace:*",
    "@storybook/addon-docs": "catalog:storybook",
    "@storybook/addon-onboarding": "catalog:storybook",
    "@storybook/react-vite": "catalog:storybook",
    "@storybook/builder-vite": "catalog:storybook",
    "@tailwindcss/typography": "catalog:tailwindcss",
    "@types/node": "^24.9.1",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "9.38.0",
    "eslint-plugin-storybook": "catalog:storybook",
    "postcss": "^8.5.6",
    "react": "catalog:react19",
    "react-dom": "catalog:react19",
    "storybook": "catalog:storybook",
    "tailwindcss": "catalog:tailwindcss",
    "tsup": "catalog:typescript",
    "tw-animate-css": "catalog:tailwindcss",
    "@tailwindcss/vite": "catalog:tailwindcss",
    "typescript": "catalog:typescript",
    "vite": "^7.1.12",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./theme.css": "./src/tailwind/theme.css",
    "./components/checkbox": {
      "import": "./src/components/checkbox.tsx",
      "types": "./src/components/checkbox.tsx"
    },
    "./components/button": {
      "import": "./src/components/button.tsx",
      "types": "./src/components/button.tsx"
    },
    "./components/card": {
      "import": "./src/components/card.tsx",
      "types": "./src/components/card.tsx"
    }
  },
  "typesVersions": {
    "*": {
      "button": [
        "src/components/button.tsx"
      ],
      "checkbox": [
        "src/components/checkbox.tsx"
      ],
      "card": [
        "src/components/card.tsx"
      ],
      "components/checkbox": [
        "src/components/checkbox.tsx"
      ],
      "components/button": [
        "src/components/button.tsx"
      ],
      "components/card": [
        "src/components/card.tsx"
      ]
    }
  }
}