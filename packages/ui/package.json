{
  "name": "@remix-gospel-stack/ui",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:tsup": "tsup --watch",
    "dev:vite": "vite",
    "build": "tsup",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "rm -rf dist",
    "lint": "eslint ./src",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.428.0",
    "npm-run-all2": "^6.2.2",
    "tailwind-merge": "^2.3.0"
  },
  "peerDependencies": {
    "@remix-run/dev": "2.11.2",
    "@remix-run/node": "2.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.2",
    "tailwindcss-animate": "^1.0.6"
  },
  "devDependencies": {
    "@remix-gospel-stack/eslint-config": "workspace:*",
    "@remix-gospel-stack/tsconfig": "workspace:*",
    "@remix-run/dev": "2.11.2",
    "@remix-run/node": "2.11.2",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/addon-onboarding": "^8.1.11",
    "@storybook/blocks": "^8.1.11",
    "@storybook/cli": "^8.1.11",
    "@storybook/react": "^8.1.11",
    "@storybook/react-vite": "^8.1.11",
    "@storybook/test": "^8.1.11",
    "@tailwindcss/typography": "^0.5.13",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "9.9.0",
    "eslint-plugin-storybook": "^0.8.0",
    "postcss": "^8.4.39",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.1.11",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2",
    "vite": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts",
      "default": "./dist/index.mjs"
    },
    "./tailwind": {
      "import": "./src/tailwind/index.ts",
      "types": "./src/tailwind/index.ts",
      "default": "./dist/tailwind/index.mjs"
    },
    "./components/checkbox": {
      "import": "./src/components/checkbox.tsx",
      "types": "./src/components/checkbox.tsx"
    },
    "./components/button": {
      "import": "./src/components/button.tsx",
      "types": "./src/components/button.tsx"
    },
    "./components/card": {
      "import": "./src/components/card.tsx",
      "types": "./src/components/card.tsx"
    }
  },
  "typesVersions": {
    "*": {
      "button": [
        "src/components/button.tsx"
      ],
      "checkbox": [
        "src/components/checkbox.tsx"
      ],
      "card": [
        "src/components/card.tsx"
      ],
      "components/checkbox": [
        "src/components/checkbox.tsx"
      ],
      "components/button": [
        "src/components/button.tsx"
      ],
      "components/card": [
        "src/components/card.tsx"
      ]
    }
  }
}