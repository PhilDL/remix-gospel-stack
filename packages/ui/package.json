{
  "name": "@remix-gospel-stack/ui",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint ./src --fix",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.292.0",
    "tailwind-merge": "^2.0.0"
  },
  "peerDependencies": {
    "@remix-run/dev": "^2.0.0",
    "@remix-run/node": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.2",
    "tailwindcss-animate": "^1.0.6"
  },
  "devDependencies": {
    "@remix-gospel-stack/eslint-config": "workspace:*",
    "@remix-gospel-stack/tsconfig": "workspace:*",
    "@remix-run/dev": "^2.2.0",
    "@remix-run/node": "^2.2.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.5",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.0.0",
    "typescript": "^5.2.2",
    "eslint-plugin-storybook": "^0.6.15",
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-interactions": "^7.6.3",
    "@storybook/addon-links": "^7.6.3",
    "@storybook/addon-onboarding": "^1.0.9",
    "@storybook/blocks": "^7.6.3",
    "@storybook/cli": "^7.6.3",
    "@storybook/react": "^7.6.3",
    "@storybook/react-vite": "^7.6.3",
    "@storybook/test": "^7.6.3",
    "storybook": "^7.6.3"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts",
      "default": "./dist/index.mjs"
    },
    "./tailwind": {
      "import": "./src/tailwind/index.ts",
      "types": "./src/tailwind/index.ts",
      "default": "./dist/tailwind/index.mjs"
    },
    "./checkbox": {
      "import": "./src/checkbox.tsx",
      "types": "./src/checkbox.tsx"
    },
    "./button": {
      "import": "./src/button.tsx",
      "types": "./src/button.tsx"
    },
    "./card": {
      "import": "./src/card.tsx",
      "types": "./src/card.tsx"
    }
  },
  "typesVersions": {
    "*": {
      "button": [
        "src/button.tsx"
      ],
      "checkbox": [
        "src/checkbox.tsx"
      ],
      "card": [
        "src/card.tsx"
      ]
    }
  }
}
