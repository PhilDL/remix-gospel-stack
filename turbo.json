{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [".env"],
  "globalEnv": [
    "NODE_ENV",
    "APP_PORT",
    "FLY_REGION",
    "PRIMARY_REGION",
    "CI",
    "PORT",
    "MOCKS",
    "DATABASE_URL",
    "DATABASE_PATH",
    "LITEFS_DIR",
    "SENTRY_DSN",
    "ALLOW_INDEXING"
  ],
  "globalPassThroughEnv": [
    "MODE",
    "DEV",
    "NODE_ENV",
    "DEBUG_MOCKS",
    "PLAYWRIGHT_TEST_BASE_URL"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build", "^db:generate"],
      "inputs": ["$TURBO_DEFAULT$", ".env*", "./other/Dockerfile"],
      "outputs": [
        "dist/**",
        ".next/**",
        "public/build/**",
        "build/**",
        "app/styles/tailwind.css",
        ".cache"
      ]
    },
    "@react-router-gospel-stack/webapp#start": {
      "dependsOn": ["^build"],
      "outputs": ["public/build/**"]
    },
    "start": {
      "dependsOn": ["^build"],
      "outputs": ["public/build/**"]
    },
    "db:migrate:deploy": {
      "outputs": []
    },
    "db:migrate:dev": {
      "outputs": []
    },
    "db:push": {
      "outputs": []
    },
    "db:seed": {
      "outputs": [],
      "cache": false
    },
    "db:migrate:force": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "storybook": {
      "outputs": [],
      "persistent": false
    },
    "db:generate": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": ["node_modules/.cache/.eslintcache"]
    },
    "docker:build": {
      "outputs": [],
      "cache": false
    },
    "test": {
      "outputs": []
    },
    "test:dev": {
      "outputs": [],
      "cache": false,
      "persistent": true
    },
    "test:e2e:dev": {
      "dependsOn": ["^build"],
      "outputs": [],
      "cache": false,
      "persistent": true
    },
    "test:e2e:ci": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "clean": {
      "cache": false
    },
    "nuke": {
      "cache": false
    }
  }
}
