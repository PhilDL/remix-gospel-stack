{
  "include": [
    ".react-router/types/**/*",
    "types/env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "index.js"
  ],
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "target": "ESNext",
    "strict": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowImportingTsExtensions": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./app/*"],
      "@react-router-gospel-stack/ui/*": ["../../packages/ui/src/*"],
      "@react-router-gospel-stack/ui": ["../../packages/ui/src/index"],
      "@react-router-gospel-stack/business/*": [
        "../../packages/business/src/*"
      ],
      "@react-router-gospel-stack/business": [
        "../../packages/business/src/index"
      ],
      "@react-router-gospel-stack/database/*": [
        "../../packages/database/src/*"
      ],
      "@react-router-gospel-stack/database": [
        "../../packages/database/src/index"
      ],
      "@react-router-gospel-stack/internal-nobuild": [
        "../../packages/internal-nobuild/src/index"
      ],
      "@react-router-gospel-stack/internal-nobuild/*": [
        "../../packages/internal-nobuild/src/*"
      ],
      "@react-router-gospel-stack/tailwind-config": [
        "../../config/tailwind-config/src/index"
      ]
    },
    "noEmit": true,
    "types": ["@react-router/node", "vite/client"],
    "rootDirs": [".", "./.react-router/types"]
  }
}
