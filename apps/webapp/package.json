{
  "private": true,
  "name": "@react-router-gospel-stack/webapp",
  "version": "1.0.0",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "react-router build",
    "dev": "NODE_ENV=development react-router dev",
    "start": "cross-env NODE_ENV=production node .",
    "start:mocks": "cross-env NODE_ENV=production MOCKS=true tsx .",
    "docker:build": "cd ../.. & docker build -t react-router-gospel-stack-webapp -f ./apps/webapp/other/Dockerfile .",
    "lint": "eslint .",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:e2e": "npm run test:e2e:dev --silent",
    "test:e2e:dev": "playwright test --ui",
    "pretest:e2e:run": "pnpm run build",
    "test:e2e:run": "cross-env CI=true playwright test",
    "test:e2e:install": "npx playwright install --with-deps chromium",
    "test:e2e:ci": "pnpm run test:e2e:install && pnpm run test:e2e:run",
    "typecheck": "react-router typegen && tsc -b",
    "with-root-env": "dotenv -e ../../.env --",
    "clean": "pnpm rimraf .turbo build public/build",
    "nuke": "pnpm rimraf .turbo build public/build node_modules coverage playwright-report test-results"
  },
  "dependencies": {
    "@epic-web/remember": "^1.1.0",
    "@react-router-gospel-stack/business": "workspace:*",
    "@react-router-gospel-stack/infrastructure": "workspace:*",
    "@react-router-gospel-stack/web-utils": "workspace:*",
    "@react-router-gospel-stack/ui": "workspace:*",
    "@react-router/node": "catalog:react-router",
    "@sentry/profiling-node": "catalog:sentry",
    "@sentry/react-router": "catalog:sentry",
    "@t3-oss/env-core": "catalog:",
    "chalk": "catalog:",
    "cross-env": "catalog:",
    "dotenv": "catalog:",
    "hono": "catalog:hono",
    "hono-rate-limiter": "catalog:hono",
    "isbot": "catalog:",
    "react": "catalog:react19",
    "react-dom": "catalog:react19",
    "react-router": "catalog:react-router",
    "react-router-dom": "catalog:react-router",
    "react-router-hono-server": "catalog:hono",
    "remix-hono": "catalog:hono",
    "source-map-support": "catalog:"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@playwright/test": "^1.56.1",
    "@react-router-gospel-stack/eslint-config": "workspace:*",
    "@react-router/dev": "catalog:react-router",
    "@sentry/vite-plugin": "catalog:sentry",
    "@tailwindcss/typography": "catalog:tailwindcss",
    "@tailwindcss/vite": "catalog:tailwindcss",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/eslint": "^9.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.0",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "@types/source-map-support": "catalog:",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.8",
    "concurrently": "^9.2.1",
    "esbuild": "0.27.3",
    "eslint": "9.39.2",
    "glob": "catalog:",
    "happy-dom": "^20.0.10",
    "msw": "^2.12.0",
    "npm-run-all2": "^8.0.4",
    "rimraf": "catalog:",
    "tailwindcss": "catalog:tailwindcss",
    "tsx": "catalog:typescript",
    "tw-animate-css": "catalog:tailwindcss",
    "typescript": "catalog:typescript",
    "vite": "^7.2.2",
    "vite-tsconfig-paths": "^6.0.0",
    "vitest": "^4.0.8"
  },
  "engines": {
    "node": "22"
  }
}
